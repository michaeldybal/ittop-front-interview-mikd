{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "globals": {
      "document": true,
      "window": true,
      "App": true,
      "google": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "modules": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "strict": [2, "never"],
        
        "no-shadow": 1,
        "no-shadow-restricted-names": 1,
        "no-unused-vars": [1, {
          "vars": "local",
          "args": "after-used"
        }],
        "no-use-before-define": 1,
        "no-undef": 1,

        "comma-dangle": [2, "never"],
        "no-cond-assign": [2, "always"],
        "no-console": 1,
        "no-debugger": 1,
        "no-alert": 1,
        "no-constant-condition": 1,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "quote-props": [2, "as-needed"],
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "block-scoped-var": 2,
        "consistent-return": 2,
        "curly": [2, "multi-line"],
        "default-case": 2,
        "dot-notation": [2, {
          "allowKeywords": true
        }],
        "eqeqeq": 2,
        "guard-for-in": 0,
        "no-caller": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 1,
        "wrap-iife": [2, "any"],
        "yoda": 2,

        "brace-style": [2,
          "1tbs", {
            "allowSingleLine": true
          }],
        "comma-spacing": [1, {
          "before": false,
          "after": true
        }],
        "comma-style": [1, "last"],
        "eol-last": 1,
        "func-names": 1,
        "key-spacing": [1, {
          "beforeColon": false,
          "afterColon": true
        }],


        "new-cap": [2, {
          "newIsCap": true,
          "capIsNew": false
        }],
        "no-multiple-empty-lines": [1, {
          "max": 2
        }],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "one-var": [2, "never"],
        "padded-blocks": 0,
        "semi": [1, "always"],
        "semi-spacing": [1, {
          "before": false,
          "after": true
        }],
        "keyword-spacing": 1,
        "space-before-blocks": 1,
        "space-before-function-paren": [1, "never"],
        "space-infix-ops": 1,
        "spaced-comment": [1, "always"],


        "indent": [
            "warn",
            2
        ]
    }
}